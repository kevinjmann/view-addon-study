{
  "name": "view",
  "version": "1.0.3063",
  "license": "MIT",
  "repository": "https://git.aleks.bg/view/view",
  "description": "VIEW testing javascript packages",
  "engines": {
    "firefox": ">=48.0a1"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "3.12.2",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-mocha": "4.7.0",
    "eslint-plugin-promise": "3.4.0",
    "fs-promise": "1.0.0",
    "fx-runner": "1.0.6",
    "geckodriver": "1.2.0",
    "grunt": "^1.0.1",
    "grunt-karma": "^2.0.0",
    "karma": "1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "1.1.1",
    "karma-coveralls": "1.1.2",
    "karma-firefox-launcher": "1.0.0",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "selenium-webdriver": "3.0.1",
    "sinon": "1.17.6",
    "sinon-chrome": "2.1.2",
    "virtualenv": "0.3.1",
    "web-ext": "1.6.0"
  },
  "permissions": {
    "multiprocess": true
  },
  "preferences": [
    {
      "name": "buttonUrl",
      "title": "Web page url",
      "description": "The web page url to open when button is clicked",
      "type": "string",
      "value": "https://www.mozilla.org/"
    }
  ],
  "scripts": {
    "bundle": "web-ext -s ../viewWE -a dist build",
    "download": "node scripts/get_ff.js",
    "firefox": "web-ext -s ../viewWE run --firefox-binary ${FIREFOX_BINARY:-firefox}",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ext=.js,.json .",
    "lint:webext": "web-ext -s ../viewWE lint",
    "test": "npm-run-all test:*",
    "test:karma": "grunt karma",
    "test:func": "export FIREFOX_BINARY=${FIREFOX_BINARY:-firefox} && npm run bundle && XPI_NAME=dist/view-1.0.3063.zip mocha test/functional/",
    "virtualenv-postinstall": "virtualenv-postinstall"
  },
  "keywords": [
    "webextensions"
  ]
}
